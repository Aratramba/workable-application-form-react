  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-visually-hidden": "^1.0.2",
    "intl-tel-input": "^17.0.19",
    "react-dropzone": "^14.2.3"
  }
