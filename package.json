{
  "name": "workable-application-form-react",
  "version": "0.1.0",
  "description": "Workable application form React renderer",
  "source": "src/index.ts",
  "main": "output/main.js",
  "module": "output/modern.main.js",
  "context": "browser",
  "scripts": {
    "build": "parcel build",
    "dev": "parcel serve examples/*.html",
    "test": "NODE_ENV=test jest"
  },
  "targets": {
    "main": {
      "optimize": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aratramba/workable-application-form-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aratramba/workable-application-form-react/issues"
  },
  "homepage": "https://github.com/Aratramba/workable-application-form-react#readme",
  "devDependencies": {
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "typescript": ">=3.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "node_modules/variables/.+\\.(j|t)sx?$": "ts-jest"
